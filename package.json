    "npm-check-updates": "^16.14.20",
    "vue-router": "^4.3.2",
    "vue3-carousel": "^0.3.3",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@rushstack/eslint-patch": "^1.10.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "autoprefixer": "^10.4.19",
    "electron": "^30.0.6",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.2.0",
